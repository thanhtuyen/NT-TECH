<?xml version="1.0" encoding="UTF-8" ?>

<themeeditor>
	<version value="1.4">
	<ccc available="true" />
	<guest_checkout available="true" />
	<one_page_checkout available="true" />
	<store_locator available="true" />
	</version>
	<author>themelexus</author>
<name>Live Theme Editor</name>	
<selectors>
	<items>
			<header>Body Content</header>
			<match>body</match>
			<item>
				<type>text</type>
				<label>Border Color</label>
				<selector>.box .box-heading,#pav-promotion</selector>
				<attrs>border-color</attrs>
			</item>
			
			<item>
				<type>text</type>
				<label>Background Color</label>
				<selector>body</selector>
				<attrs>background-color</attrs>
			</item>

			<item>
			<type>image</type>
				<label>Background Image</label>
				<selector>body</selector>
				<attrs>background-image</attrs>
			</item> 

		 <item>
			<type>text</type>
			<label>Background Content</label>
			<selector>body #page,[class*="layout-boxed-"] #page</selector>
			<attrs>background-color</attrs>
		</item> 
			<item>
				<type>image</type>
				<label>Background Image</label>
				<selector>body #page,[class*="layout-boxed-"] #page</selector>
				<attrs>background-image</attrs>
			</item> 

	 	<item>
			<type>fontsize</type>
			<label>Font-Size</label>
			<selector>body #page</selector>
			<attrs>font-size</attrs>
		</item> 


		<item>
			<type>text</type>
			<label>Color Text</label>
			<selector>body #page</selector>
			<attrs>color</attrs>
		</item> 
		<item>
			<type>text</type>
			<label>Link Color</label>
			<selector>body #page a</selector>
			<attrs>color</attrs>
		</item> 
	</items>
 
	<items>
		<header>TopBar</header>
		<match>topbar</match>
		<item>
			<type>text</type>
			<label>Background Color</label>
			<selector>#topbar</selector>
			<attrs>background-color</attrs>
		</item> 
	 	<item>
			<type>text</type>
			<label>Color Text</label>
			<selector>#topbar,#topbar .btn-dropdown span</selector>
			<attrs>color</attrs>
		</item> 
	 	<item>
			<type>text</type>
			<label>Color Link</label>
			<selector>#topbar a</selector>
			<attrs>color</attrs>
		</item> 
		<item>
			<type>text</type>
			<label>Border Color</label>
			<selector>#topbar,#topbar .btn-dropdown</selector>
			<attrs>border-color</attrs>
		</item>
		<item>
			<type>text</type>
			<label>Icon Color</label>
			<selector>#topbar i</selector>
			<attrs>color</attrs>
		</item>
	</items>
	<items>
		<header>Header</header>
		<match>header-main</match>
		<item>
			<type>text</type>
			<label>Background Color</label>
			<selector>#header-main</selector>
			<attrs>background-color</attrs>
		</item> 
		<item>
			<type>image</type>
			<label>Background Image</label>
			<selector>#header-main</selector>
			<attrs>background-image</attrs>
		</item>
	</items>
	<items>
		<header>MainMenu</header>
		<match>pav-mainnav</match>		
		<item>
			<type>text</type>
			<label>Color Text</label>
			<selector>#pav-mainnav p, #pav-mainnav .pavo-widget .menu-title, #pav-mainnav h1, #pav-mainnav h2, #pav-mainnav h3, #pav-mainnav h4, #pav-mainnav h5, #pav-mainnav h6</selector>
			<attrs>color</attrs>
		</item>
	 	<item>
			<type>text</type>
			<label>Color Link</label>
			<selector>.pav-megamenu .navbar-nav li a</selector>
			<attrs>color</attrs>
		</item>
		<item>
			<type>text</type>
			<label>Color Link Hover</label>
			<selector><![CDATA[.pav-megamenu .navbar-nav li a:hover]]></selector>
			<attrs>color</attrs>
		</item> 
		<item>
			<type>text</type>
			<label>Sub Color Link</label>
			<selector>#pav-mainnav .dropdown-menu li a, #pav-mainnav .dropdown-menu a</selector>
			<attrs>color</attrs>
		</item> 		
	</items>
	 
	<items>
		<header>Footer Top</header>
		<match>footer-top</match>
		<item>
			<type>text</type>
			<label>Background Color</label>
			<selector>.footer-top</selector>
			<attrs>background-color</attrs>
		</item> 		
		<item>
			<type>text</type>
			<label>Border Color</label>
			<selector>#footer .footer-top .custom</selector>
			<attrs>border-color</attrs>
		</item>
		<item>
			<type>text</type>
			<label>Icon Color</label>
			<selector>#footer .footer-top .carousel-control</selector>
			<attrs>color</attrs>
		</item> 
		<item>
			<type>image</type>
			<label>Background Image</label>
			<selector>.footer-top</selector>
			<attrs>background-image</attrs>
		</item>
	</items>
	<items>
		<header>Footer Center</header>
		<match>footer-center</match>
			<item>
				<type>text</type>
				<label>Background Color</label>
				<selector>.footer-center</selector>
				<attrs>background-color</attrs>
			</item> 
			<item>
				<type>text</type>
				<label>Color Text</label>
				<selector>.footer-center, #footer .footer-center .box .box-heading, #footer .footer-center .has-highlight label</selector>
				<attrs>color</attrs>
			</item> 
			<item>
				<type>text</type>
				<label>Color Link</label>
				<selector>.footer-center a</selector>
				<attrs>color</attrs>
			</item>
			<item>
				<type>text</type>
				<label>Border Color</label>
				<selector>#footer .box .box-heading,#footer .box .box-heading span</selector>
				<attrs>border-color</attrs>
			</item> 
			<item>
				<type>text</type>
				<label>Bg Icon</label>
				<selector>#footer .social li</selector>
				<attrs>background-color</attrs>
			</item> 
			<item>
				<type>text</type>
				<label>Color Icon</label>
				<selector>#footer .social li a,#footer .icon-newsletter,#footer .icon-bullet-right, #footer ul.contact-us li i</selector>
				<attrs>color</attrs>
			</item> 
			<item>
				<type>text</type>
				<label>Border Icon</label>
				<selector>#footer .social li</selector>
				<attrs>border-color</attrs>
			</item> 
			<item>
				<type>text</type>
				<label>Bg Input</label>
				<selector>#footer .footer-center input, #footer .footer-center .form-control</selector>
				<attrs>background-color</attrs>
			</item>
			<item>
				<type>text</type>
				<label>Input Color</label>
				<selector>#footer .footer-center input, #footer .footer-center .form-control</selector>
				<attrs>color</attrs>
			</item> 
			<item>
				<type>image</type>
				<label>Background Image</label>
				<selector>.footer-center</selector>
				<attrs>background-image</attrs>
			</item>
	</items>
	<items>
		<header>Copyright</header>
		<match>powered</match>
		<item>
			<type>text</type>
			<label>Background Color</label>
			<selector>#powered</selector>
			<attrs>background-color</attrs>
		</item> 
		<item>
			<type>text</type>
			<label>Color Text</label>
			<selector>#powered</selector>
			<attrs>color</attrs>
		</item> 
	 	<item>
			<type>text</type>
			<label>Color Link</label>
			<selector>#powered a</selector>
			<attrs>color</attrs>
		</item> 
		<item>
			<type>text</type>
			<label>Border Color</label>
			<selector>#powered</selector>
			<attrs>border-color</attrs>
		</item>
		<item>
			<type>text</type>
			<label>Icon Color</label>
			<selector>#back-top a</selector>
			<attrs>color</attrs>
		</item>
		<item>
			<type>text</type>
			<label>Border Icon</label>
			<selector>#back-top a</selector>
			<attrs>border-color</attrs>
		</item>
		<item>
			<type>text</type>
			<label>Bg Icon</label>
			<selector>#back-top a</selector>
			<attrs>background-color</attrs>
		</item>
		<item>
			<type>image</type>
			<label>Background Image</label>
			<selector>#powered</selector>
			<attrs>background-image</attrs>
		</item> 
	</items>
</selectors>

<elements>
	<items>
		<header>Products</header>
		<match>product</match>
		<item>
			<type>text</type>
			<label>Product Name</label>
			<selector>.product-block .name a</selector>
			<attrs>color</attrs>
		</item> 
		<item>
			<type>text</type>
			<label>Color Price New</label>
			<selector>.price .price-new</selector>
			<attrs>color</attrs>
		</item>  		
 		<item>
			<type>text</type>
			<label>Color Price Old</label>
			<selector>.price .price-old</selector>
			<attrs>color</attrs>
		</item> 
		<item>
			<type>text</type>
			<label>Bg Add To Cart</label>
			<selector>.cart input</selector>
			<attrs>background-color</attrs>
		</item>
		<item>
			<type>text</type>
			<label>Color Add To Cart</label>
			<selector>.cart input</selector>
			<attrs>color</attrs>
		</item> 
		<item>
			<type>text</type>
			<label>Bg Icon</label>
			<selector>.product-block .product-icon,.product-block .image .product-zoom</selector>
			<attrs>background-color</attrs>
		</item>
		<item>
			<type>text</type>
			<label>Color Icon</label>
			<selector>.product-block .product-icon,.product-block .image .product-zoom</selector>
			<attrs>color</attrs>
		</item>
		<item>
			<type>text</type>
			<label>Border Icon</label>
			<selector>.product-block .product-icon</selector>
			<attrs>border-color</attrs>
		</item>
		<item>
			<type>text</type>
			<label>Bg Sale</label>
			<selector>.product-label-special</selector>
			<attrs>background-color</attrs>
		</item>
		<item>
			<type>text</type>
			<label>Color Sale</label>
			<selector>.product-label-special</selector>
			<attrs>color</attrs>
		</item>
	</items>
	<items>
		<header>Search Bar</header>
		<match>header-bottom</match>
		<item>
			<type>text</type>
			<label>Bg Color</label>
			<selector>#header-bottom</selector>
			<attrs>background-color</attrs>
		</item>
		<item>
			<type>text</type>
			<label>Bg Color Highlight</label>
			<selector>.pav-dropdown .d-heading</selector>
			<attrs>background-color</attrs>
		</item>
		<item>
			<type>text</type>
			<label>Text Color</label>
			<selector>.pav-dropdown .d-heading h4, #header #cart .heading h4</selector>
			<attrs>color</attrs>
		</item>
		<item>
			<type>text</type>
			<label>Link Color</label>
			<selector>#header #cart .heading a</selector>
			<attrs>color</attrs>
		</item>
		<item>
			<type>text</type>
			<label>Input Color</label>
			<selector>.input-search</selector>
			<attrs>color</attrs>
		</item>
		<item>
			<type>text</type>
			<label>Border Color</label>
			<selector>.input-search,.pav-dropdown .d-content</selector>
			<attrs>border-color</attrs>
		</item>
		<item>
			<type>text</type>
			<label>Bg Icon</label>
			<selector>.pav-dropdown .d-heading h4 > span</selector>
			<attrs>background-color</attrs>
		</item>
		<item>
			<type>text</type>
			<label>Icon Color</label>
			<selector>#header #cart .heading i</selector>
			<attrs>color</attrs>
		</item>		
		<item>
			<type>text</type>
			<label>Border Icon</label>
			<selector>#header #cart .heading i</selector>
			<attrs>border-color</attrs>
		</item>		
	</items>

	<items>
		<header>Default Modules Sidebar</header>
		<match>modules</match>
		<item>
			<type>text</type>
			<label>Heading Color</label>
			<selector>.sidebar .box .box-heading span</selector>
			<attrs>color</attrs>
		</item> 
		<item>
			<type>text</type>
			<label>BgContent</label>
			<selector>.sidebar .box .box-content</selector>
			<attrs>background-color</attrs>
		</item>  
		<item>
			<type>text</type>
			<label>Content Color</label>
			<selector>.sidebar .box .box-content</selector>
			<attrs>color</attrs>
		</item> 
		<item>
			<type>text</type>
			<label>Link Color</label>
			<selector>.sidebar .product-block .name a, .sidebar ul li a</selector>
			<attrs>color</attrs>
		</item>
		<item>
			<type>text</type>
			<label>Border Color</label>
			<selector>.sidebar .productdeals .product-block,.sidebar .box .box-heading,.sidebar .product-block,.sidebar ul > li</selector>
			<attrs>border-color</attrs>
		</item> 
	</items>

	<items>
		<header>Highlight Modules Sidebar</header>
		<match>highlight-modules</match>
		<item>
			<type>text</type>
			<label>BgHeading</label>
			<selector>.sidebar .box.highlights .box-heading, .sidebar .box-highlight .box-heading</selector>			
			<attrs>background-color</attrs>
		</item> 
		<item>
			<type>text</type>
			<label>Heading Color</label>
			<selector>.sidebar .box.highlights .box-heading span, .sidebar .box-highlight .box-heading span</selector>
			<attrs>color</attrs>
		</item> 
		<item>
			<type>text</type>
			<label>BgContent</label>
			<selector>.sidebar .box.highlights .box-content, .sidebar .box-highlight .box-content, .sidebar ul > li > ul > li</selector>
			<attrs>background-color</attrs>
		</item>  
		<item>
			<type>text</type>
			<label>Content Color</label>
			<selector>.sidebar .box.highlights .box-content,.sidebar .box-highlight .box-content</selector>
			<attrs>color</attrs>
		</item> 
		<item>
			<type>text</type>
			<label>Link Color</label>
			<selector>.sidebar .box.highlights .box-content a, .sidebar .box-highlight .box-content a</selector>
			<attrs>color</attrs>
		</item>
		<item>
			<type>text</type>
			<label>Border Color</label>
			<selector>.sidebar .highlights ul > li,.sidebar .highlights .box-heading</selector>
			<attrs>border-bottom-color</attrs>
		</item>			

	</items>
	
</elements>
</themeeditor>
